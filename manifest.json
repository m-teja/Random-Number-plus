{
    "manifest_version": 2,
    "name": "Rock Paper Scissors",
    "version": "0.1",
    "author": "Matthew Teja", 
    "browser_action": { 
        "default_popup": "popup.html"
    },
    "permissions": 
    ["storage", "notifications"],
    
    "content_scripts":[
        {
        "matches": 
        [
        "<all_urls>"
        ],
        "js": ["main.js"],
        "css": ["popup.css"]
        }    
    ]
}
