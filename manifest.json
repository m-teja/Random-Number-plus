{
    "manifest_version": 2,
    "name": "Random Number Plus",
    "version": "0.1",
    "author": "Matthew Teja", 
    "browser_action": { 
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "permissions": 
    ["storage", "notifications"],
    
    "content_scripts":[
        {
        "matches": 
        [
        "<all_urls>"
        ],
        "js": [],
        "css": ["popup.css"]
        }    
    ]
}
